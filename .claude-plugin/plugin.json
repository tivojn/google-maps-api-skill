{
  "name": "google-maps-api",
  "description": "Complete Google Maps Platform API client - 20+ APIs for geocoding, routes, places, weather, air quality, pollen, solar, elevation, timezone, address validation, roads, street view, geolocation, and more",
  "version": "1.0.0",
  "author": "Custom",
  "category": "google",
  "permissions": ["env-access"],
  "triggers": [
    "google maps", "maps api", "geocode", "directions", "places", "weather",
    "air quality", "pollen", "solar", "elevation", "timezone", "address validation",
    "street view", "static map", "geolocation", "route optimization", "distance matrix",
    "aerial view", "nearby places", "pollen index", "traffic"
  ],
  "requiredEnvVars": ["GOOGLE_MAPS_API_KEY"],
  "capabilities": {
    "geocoding": true,
    "reverseGeocoding": true,
    "directions": true,
    "distanceMatrix": true,
    "placesSearch": true,
    "placesNearby": true,
    "placeDetails": true,
    "autocomplete": true,
    "placePhotos": true,
    "elevation": true,
    "timezone": true,
    "airQuality": true,
    "airQualityHistory": true,
    "airQualityForecast": true,
    "pollen": true,
    "solar": true,
    "solarLayers": true,
    "weather": true,
    "weatherForecast": true,
    "addressValidation": true,
    "roadsSnap": true,
    "roadsNearest": true,
    "streetView": true,
    "staticMaps": true,
    "geolocation": true,
    "aerialView": true,
    "routeOptimization": true,
    "placesAggregate": true,
    "mapsEmbed": true
  }
}
